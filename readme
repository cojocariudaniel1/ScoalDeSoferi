pyuic5 -x file_name.ui -o gui.py

if __name__ == "__main__":
    import sys
    app = QtWidgets.QApplication(sys.argv)
    Form = QtWidgets.QWidget()
    ui = Ui_Form()
    ui.setupUi(Form)
    Form.show()
    sys.exit(app.exec_())

    def login_buttn_func(self):
        if login_check(self.username_input.text(),
                       self.password_input.text()):
            self.run_user_form()

    def run_user_form(self):
        self.MainWindow_VAR.hide()
        self.FormWidget = QtWidgets.QMainWindow()
        self.ui = UserForm(self.username_input.text())
        try:
            self.ui.setupUi(self.FormWidget)
        except Exception as e:
            print(e)
        self.FormWidget.show()
        print('run user form')